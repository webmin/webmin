log_apply=Değişiklikler uygulandı
log_create_serv=$1 servisi oluşturuldu
log_modify_serv=$1 servisi değiştirildi
log_delete_serv=$1 servisi silindi
log_create_prog=$1 programı ve servisi oluşturuldu
log_modify_prog=$1 programı ve servisi değiştirildi
log_delete_prog=$1 programı ve servisi silindi
log_create_serv_l=Port $2'deki $1 servisi oluşturuldu
log_modify_serv_l=Port $2'deki $1 servisi değiştirildi
log_delete_serv_l=Port $2'deki $1 servisi silindi
log_create_prog_l=Program $3 için port $2'deki $1 servisi oluşturuldu
log_modify_prog_l=Program $3 için port $2'deki $1 servisi değiştirildi
log_delete_prog_l=Program $3 için port $2'deki $1 servisi silindi

log_create_rpc=RPC $1 oluşturuldu
log_modify_rpc=RPC $1 değiştirildi
log_delete_rpc=RPC $1 silindi
log_create_rprog=$1 programı ve RPC oluşturuldu
log_modify_rprog=$1 programı ve RPC değiştirildi
log_delete_rprog=$1 programı ve RPC silindi
log_create_rpc_l=RPC $1 numara $2 oluşturuldu
log_modify_rpc_l=RPC $1 numara $2 değiştirildi
log_delete_rpc_l=RPC $1 numara $2 silindi
log_create_rprog_l=$3 programı için RPC $1 numara $2 oluşturuldu
log_modify_rprog_l=$3 programı için RPC $1 numara $2 değiştirildi
log_delete_rprog_l=$3 programı için RPC $1 numara $2 silindi
