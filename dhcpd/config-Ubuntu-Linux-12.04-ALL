dhcpd_conf=/etc/dhcp/dhcpd.conf
pid_file=/var/run/dhcpd.pid /run/dhcp-server/dhcpd.pid
dhcpd_path=/usr/sbin/dhcpd
lease_file=/var/lib/dhcp/dhcpd.leases
lease_sort=0
dhcpd_nocols=5
lease_tz=0
start_cmd=service isc-dhcp-server start
stop_cmd=service isc-dhcp-server stop
restart_cmd=service isc-dhcp-server restart
show_ip=0
show_mac=0
group_name=0
display_max=100
desc_name=0
interfaces_type=debian
list_listen_interfaces=0
