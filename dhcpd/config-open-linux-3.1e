dhcpd_conf=/etc/dhcp/dhcpd.conf
pid_file=/var/run/dhcpd.pid
dhcpd_path=/usr/sbin/dhcpd
lease_file=/etc/dhcpd.leases
lease_sort=0
dhcpd_nocols=5
lease_tz=0
start_cmd=/etc/rc.d/init.d/dhcpd start
stop_cmd=/etc/rc.d/init.d/dhcpd stop
restart_cmd=/etc/rc.d/init.d/dhcpd reload
interfaces_type=caldera
show_ip=0
show_mac=0
group_name=0
display_max=100
desc_name=0
