start_cmd=/etc/init.d/dhcpd start
stop_cmd=/etc/init.d/dhcpd stop
restart_cmd=/etc/init.d/dhcpd restart
dhcpd_conf=/etc/dhcpd.conf
pid_file=/var/lib/dhcp/var/run/dhcpd.pid
dhcpd_path=/usr/sbin/dhcpd
lease_file=/var/lib/dhcp/db/dhcpd.leases
lease_sort=0
dhcpd_nocols=5
lease_tz=0
interfaces_type=suse
show_ip=0
show_mac=0
group_name=0
display_max=100
desc_name=0
list_listen_interfaces=0
